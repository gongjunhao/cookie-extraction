{
  "name": "Cookie提取助手",
  "description": "辅助抓取网站登陆后有效Cookies",
  "version": "0.0.1",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "webNavigation",
    "notifications",
    "activeTab",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
    "jquery.min.js",
    "utils.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_title": "Cookie提取助手",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}

